{
	"name": "memoize",
	"version": "1.0.0",
	"description": "This project includes the memoize function - a powerful tool for optimizing function calls. The implementation ensures that the specified function is called no more than once within a specified time interval, preventing excessive execution",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"build": "npm run clean && tsc --build tsconfig.json && npm run copy-assets",
		"build:watch": "tsc --build tsconfig.json --watch",
		"clean": "rimraf dist",
		"copy-assets": "cp -r public/* dist/",
		"test": "jest --coverage --testEnvironment=node --config=jest.config.js",
		"start": "node dist/cli/cli.js",
		"start:dev": "npx ts-node src/index.ts",
		"start:cli": "npx ts-node src/cli/cli.ts",
		"dev": "npx nodemon",
		"lint": "npx eslint ./src",
		"format": "npx eslint ./src --fix"
	},
	"keywords": [],
	"author": "Irine7",
	"license": "ISC",
	"devDependencies": {
		"@types/inquirer": "^9.0.7",
		"@types/jest": "^29.5.10",
		"@types/mocha": "^10.0.6",
		"@types/node": "^20.10.2",
		"@typescript-eslint/eslint-plugin": "^6.13.1",
		"@typescript-eslint/parser": "^6.13.1",
		"eslint": "^8.55.0",
		"jest": "^29.7.0",
		"mocha": "^10.2.0",
		"nodemon": "^3.0.2",
		"rimraf": "^5.0.5",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.3.2"
	},
	"dependencies": {
		"inquirer": "^9.2.12"
	}
}
